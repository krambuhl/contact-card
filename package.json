    "broccoli-static-compiler": "^0.2.1",
    "broccoli-watchify": "^0.2.0",
    "grunt": "^0.4.5",
    "grunt-grunticon": "^1.3.0",
    "local-web-server": "^0.5.15",
    "tape": "^3.0.3"
  },
  "dependencies": {
    "domready": "^1.0.7",
